<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>
  <system.diagnostics>
    <trace autoflush="false">
      <listeners>
        <add name="configConsoleListener" type="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
  <connectionStrings>
    <clear />
    <add name="Hltbs" connectionString="UseDevelopmentStorage=true"/>
  </connectionStrings>
  <appSettings>
    <add key="MaxDegreeOfConcurrencyFactor" value="5"/>
    <add key="ScrapingLimit" value="2147483647"/>
  </appSettings>
</configuration>